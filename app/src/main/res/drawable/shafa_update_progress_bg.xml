<?xml version="1.0" encoding="utf-8" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 背景 -->
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="24px" />

            <solid android:color="@color/shafa_update_progress_background_color" />
        </shape>
    </item>

    <item
        android:id="@android:id/secondaryProgress"
        android:drawable="@color/shafa_update_progress_secondary_color">
    </item>


    <!-- 进度条 -->
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="24px" />

                <solid android:color="@color/shafa_update_progress_progress_color" />
            </shape>
        </clip>
    </item>


</layer-list>